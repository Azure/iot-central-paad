{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "sourceMap": true,
      "jsx": "react-native",
      "lib": [
        "es6"
      ],
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "noEmit": true,
      "strict": true,
      "noUnusedLocals": true,
      "module": "commonjs",
      "target": "es2017",
      "skipLibCheck": true
    },
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js",
      "creds-generator"
    ]
  }